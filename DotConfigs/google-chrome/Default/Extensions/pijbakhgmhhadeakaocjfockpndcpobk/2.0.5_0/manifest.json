{
   "action": {
      "default_icon": {
         "16": "icon16.png"
      },
      "default_title": "YTPD Calculator"
   },
   "content_scripts": [ {
      "js": [ "src/library.js", "src/content.js" ],
      "matches": [ "https://www.youtube.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "An extension to calculate & display the total duration of a youtube playlist.",
   "host_permissions": [ "https://www.youtube.com/*" ],
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Youtube Playlist Duration Calculator",
   "short_name": "YTPD Calc",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.5"
}
